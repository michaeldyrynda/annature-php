{
    "name": "dyrynda/annature",
    "description": "A PHP integration for the Annature eSignature and client verification API",
    "type": "library",
    "require": {
        "saloonphp/saloon": "^3.7",
        "illuminate/collections": "^10.0|^11.0"
    },
    "require-dev": {
        "pestphp/pest": "^2.34",
        "laravel/pint": "^1.14"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Dyrynda\\Annature\\": "src/",
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Michael Dyrynda",
            "email": "michael@dyrynda.com.au"
        }
    ],
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "scripts": {
        "fix-style": "vendor/bin/pint",
        "test": "vendor/bin/pest -p" 
    }
}
